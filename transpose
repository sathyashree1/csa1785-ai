
 
M = 3
N = 3
 

 
def transpose(A, B):
 
 for i in range(N):
  for j in range(M):
   B[i][j] = A[j][i]
 

A = [[2,3,4],
     [5,6,7],
     [4,7,9]]
 
 

B = [[0 for x in range(M)] for y in range(N)]
 
transpose(A, B)
 
print("Result matrix is")
for i in range(N):
 for j in range(M):
  print(B[i][j], " ", end='')
 print() 
